<Window x:Class="TimeZoneDB_JSON.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TimeZoneDB_JSON"
        mc:Ignorable="d"
        Title="TimeZoneDB_JSON" Width="400" SizeToContent="Height" Loaded="Window_Loaded">
    <Grid>
        <Button x:Name="Button_CheckUpdate" Content="Check for update" Margin="10,10,10,0" VerticalAlignment="Top" Height="30" Click="Button_CheckUpdate_Click"/>
        <Button x:Name="Button_Download" Content="Download" Margin="10,45,10,0" VerticalAlignment="Top" Height="30" Click="Button_Download_Click"/>
        <Button x:Name="Button_Extract" Content="Extract" Margin="10,80,10,0" VerticalAlignment="Top" Height="30" Click="Button_Extract_Click"/>
        <Button x:Name="Button_Generate" Content="Generate" Margin="10,115,10,0" VerticalAlignment="Top" Height="30" Click="Button_Generate_Click"/>
        <Button x:Name="Button_Open" Content="OpenJson" Margin="10,150,10,10" VerticalAlignment="Top" Height="30" Click="Button_Open_Click"/>
    </Grid>
</Window>
